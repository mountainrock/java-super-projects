<?xml version="1.0"?>
<toolbox>
	<data type="string">
		<key>defaultFormat</key>
		<value>dd/MM/yyyy</value>
	</data>
	<data type="string">
		<key>monthNameFormat</key>
		<value>dd MMMMMMMMM yyyy</value>
	</data>
	<data type="string">
		<key>isoFormat</key>
		<value>yyyy-MM-dd</value>
	</data>
	<tool>
		<key>dateTool</key>
		<scope>request</scope>
		<class>org.apache.velocity.tools.generic.DateTool</class>
	</tool>
	<tool>
		<key>mathTool</key>
		<scope>request</scope>
		<class>org.apache.velocity.tools.generic.MathTool</class>
	</tool>
	<tool>
		<key>import</key>
		<scope>request</scope>
		<class>org.apache.velocity.tools.view.tools.ImportTool</class>
	</tool>
	<tool>
		<key>escapeTool</key>
		<class>org.apache.commons.lang.StringEscapeUtils</class>
	</tool>
	<tool>
	  <key>esc</key>
	  <scope>application</scope>
	  <class>org.apache.velocity.tools.generic.EscapeTool</class>
	</tool>
	
	
</toolbox>
